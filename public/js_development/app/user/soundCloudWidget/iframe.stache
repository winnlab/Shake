<iframe
        id="soundcloud_widget"
        width={{width}}
        height={{height}}
        scrolling={{scrolling}}
        frameborder={{frameborder}}
        src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/playlists/{{playlist_id}}&amp;auto_play={{auto_play}}&amp;hide_related={{hide_related}}&amp;show_comments={{show_comments}}&amp;show_user={{show_user}}&amp;show_reposts={{show_reposts}}&amp;visual={{visual}}"></iframe>